    "mongoose",
    "redis",
    "mocha",
    "chai",
    "istanbul",
    "winston",
    "docker",
    "iot",
    "internet-of-things",
    "iot-server",
    "mqtt"
  ],
  "dependencies": {
    "@google/maps": "^0.4.5",
    "async-mqtt": "^1.0.1",
    "bluebird": "^3.4.7",
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "cross-env": "^5.1.3",
