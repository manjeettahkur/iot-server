{
  "name": "iot_backend",
  "version": "0.0.1",
  "description": "ES6 NodeJS + MongoDB IoT backend",
  "author": "Mart√≠n Montes <martin11lrx@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "scripts": {
    "start": "export NODE_ENV=development && babel-node src/index.js",
    "test": "cross-env NODE_ENV=test nyc mocha --timeout 10000 test/",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rm -rf dist/*",
    "prebuild": "npm run clean",
    "build": "babel -d dist/ src/",
    "dist": "npm run build && cp package.json dist/ && cd dist && npm install --production && cd -",
    "production": "export NODE_ENV=production && node index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mmontes11/iot_backend.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "redis",
    "chai",
    "mocha",
    "es6",
    "babel",
    "REST",
    "API",
    "IoT"
  ],
  "dependencies": {
    "@google/maps": "^0.4.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "bluebird": "^3.4.7",
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "express": "4.14.0",
    "express-basic-auth": "^0.2.3",
    "express-jwt": "^5.1.0",
    "express-winston": "^2.4.0",
    "helmet": "^3.3.0",
    "http-status": "^1.0.1",
    "jsonwebtoken": "^7.2.1",
    "method-override": "^2.3.5",
    "moment": "^2.18.1",
    "mongoose": "^4.7.4",
    "mongoose-geojson-schema": "^2.1.2",
    "mongoose-schema-extend": "^0.2.2",
    "redis": "^2.6.5",
    "request-ip": "^2.0.2",
    "run-sequence": "^1.1.5",
    "underscore": "^1.8.3",
    "winston": "^2.3.0",
    "winston-mongodb": "^2.0.10"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
