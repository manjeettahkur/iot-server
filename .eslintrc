{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "mocha": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "import/no-dynamic-require": 0,
    "prettier/prettier": [
      "error",
      {
        "printWidth": 120,
        "trailingComma": "all"
      }
    ],
    "no-console": [1]
  }
}
